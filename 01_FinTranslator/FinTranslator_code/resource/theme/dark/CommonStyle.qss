/* -------------------------------- */
/* QListView */
QListView
{
  background-color: rgba(35, 38, 41, 1);
  font-size: 16px;
  font-weight: 500;
  /* font-weight: bold; */
  border-radius: 0px;
  padding: 0px;
  margin: 0px;
  border: 0px;
  
}

QListView::item
{
  padding: 6px 2px;
  min-height: 32px;
  color: rgba(255, 255, 255, 0.9);
  selection-color: rgb(220, 222, 223);
  border-color: transparent;
}

QListView::item:hover
{
  color: rgba(0, 0, 0, 0.8);

  background-color:rgba(255, 255, 255, 0.80);
  selection-background-color:rgba(255, 255, 255, 0.80);
}


/* -------------------------------- */
/* QMenu item*/
QMenu {
  background-color: rgba(37, 37, 37, 1);
  border: 1px solid rgba(100, 100, 100, 1);
  padding: 4px;
}

QMenu::item
{
  height: 24px;
  border: 8px solid transparent;
  color: rgba(255, 255, 255, 1);
  padding: 0px 8px 0px 8px;
}

QMenu::item:selected,
QListView::item:selected
{
  color: rgba(255, 255, 255, 1);
  selection-color: rgba(255, 255, 255, 1); /* For Windows */

  background-color:rgba(189, 189, 189, 0.37);
  selection-background-color:rgba(189, 189, 189, 0.37);
}

QMenu::item:disabled 
{
  color: rgba(255, 255, 255, 0.3);
} 
QMenu::separator 
{
  height: 1px;
  background-color: rgba(90, 90, 90, 1);
}

/* -------------------------------- */
/* TrayIconMenu */
QMenu#trayIconMenu
{
  border-radius: 5px;
  padding: 4px;
}
QMenu#trayIconMenu::item
{
  height: 16px;
  height: 14px;
  border: 0px solid transparent;
  border-radius: 4px;
  color: rgba(255, 255, 255, 1);
  padding: 8px 8px 8px 18px;
}

QMenu#trayIconMenu::item:disabled 
{
  color: rgba(255, 255, 255, 0.3);
} 

QMenu#trayIconMenu::icon,
QMenu#trayIconMenu::icon
{
  margin: 8px;
}

QMenu#trayIconMenu::separator,
QMenu#trayIconMenu::separator
{
  margin: 4px 0px 4px 0px ;
}


/* -------------------------------- */
/* TextEidt QMenu */
QTextEdit QMenu,
QPlainTextEdit QMenu 
{
  background-color: rgba(35, 36, 37, 1);
  border: 1px solid rgba(120, 120, 120, 1);
  border-radius: 5px;
  padding: 4px;
}

QTextEdit QMenu::item,
QPlainTextEdit QMenu::item
{
  font-size: 14px;
  height: 14px;
  border: 0px solid transparent;
  border-radius: 4px;
  color: rgba(225, 225, 225, 1);
  padding: 8px 8px 8px 18px;
}
QTextEdit QMenu::icon,
QPlainTextEdit QMenu::icon
{
  margin: 8px;
}

QTextEdit QMenu::separator,
QPlainTextEdit QMenu::separator
{
  margin: 4px 0px 4px 0px ;
}


/* -------------------------------- */
/* QComboBox */
QComboBox
{
  color: rgba(225, 225, 225, 1);
  min-height: 32px;

  border: 0px solid rgba(0, 0, 0, 0);
  border-top: 0px solid rgba(0, 0, 0, 0);
  border-bottom: 2px solid rgba(170, 170, 170, 1);

  padding: 0px 7px 0px 7px;

  background-color: rgba(35, 38, 41, 1);
}

QComboBox:hover
{
  color: rgba(245, 245, 245, 1);
  border-bottom-color: rgba(245, 245, 245, 1);
}

QComboBox:on,
QComboBox:focus
{
  border-top-width: 1px;
  border-bottom-width: 3px;
  color: rgba(255, 255, 255, 1);
  border-bottom-color: rgba(255, 255, 255, 1);
}

QComboBox:disabled 
{
  color: rgba(117, 117, 117, 1);
  background-color: rgba(44, 46, 48, 1);
  border-bottom: 2px solid #232629;
}

QComboBox::drop-down	
{
  color: #ffffff;
  width: 20px;
}

QComboBox QListView::item
{
  padding: 4px 4px;
  min-height: 28px;
}


